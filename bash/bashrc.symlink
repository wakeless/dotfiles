alias ss='open tmp/capybara/`ls tmp/capybara|grep png|tail -n 1`'
alias gl='git log --date-order --pretty="format:%C(yellow)%h%Cblue%d%Creset %s %C(dim white) %an, %ar%Creset" --graph'

# Add npm global bin to PATH on sprite
if command -v sprite-env &> /dev/null; then
  export PATH="$(npm config get prefix)/bin:$PATH"
fi

eval "$(/home/linuxbrew/.linuxbrew/bin/brew shellenv bash)"
